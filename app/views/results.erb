    </header>
    <main>
        <p><%= @number %> is <%= @numbword %> </p>
        <p><%= @numbword %> is <%= @sic %> </p>
        <p><%= @sic %> is <%= @op %> </p>
        <p><%= @op %> is <%= @result %> </p>
            <%case @result
              when 1%>
                <p>1 is 3</p>
                <p>3 is 5</p>
                <p>5 is 4</p>
                <p>4 is the magic number.</p>
            <%when 2%> 
                <p>2 is 3</p>
                <p>3 is 5</p>
                <p>5 is 4</p>
                <p>4 is the magic number.</p>
            <%when 3%>
                <p>3 is 5</p>
                <p>5 is 4</p>
                <p>4 is the magic number.</p>
            <%when 4%>
                <p>4 is 4</p>
                 <p>four is the magic number.</p>
            <%when 5%>
                <p>5 is 4</p>
                 <p>four is the magic number.</p>
            <%when 6%>
                <p>6 is 3</p>
                <p>3 is 5</p>
                <p>5 is 4</p>
                <p>4 is the magic number.</p>
            <%when 7%>
                <p>7 is 5</p>
                <p>5 is 4</p>
                <p>4 is the magic number.</p>
            <%when 8%>
                <p>8 is 5</p>
                <p>5 is 4</p>
                <p>4 is the magic number.</p>
            <%when 9%>
                <p>9 is 4</p>
                 <p>four is the magic number.</p>
            <%when 10%>
                <p>10 is 3</p>
                <p>3 is 5</p>
                <p>5 is 4</p>
                <p>4 is the magic number.</p>
            <%end%>
    </main>
