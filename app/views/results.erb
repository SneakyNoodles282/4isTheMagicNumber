        <p><%= @op %> is <%= @result %> </p>
            <%case @result
              when 1%>
                <p>1 is 3</p>
                <p>3 is 5</p>
                <p>5 is 4</p>
                <p>4 is the magic number.</p>
            <%when 2%> 
                <p>2 is 3</p>
